<h2>Movie List</h2>
{{#each movieList}}
<h1>{{this.title}}</h1>
<p>{{this.genre}}</p>
<p>{{this.description}}</p>

<a href="movies/details/{{this._id}}">
    <button>More Info!</button>
</a>

<form action="movies/delete/{{this._id}}" method="POST">
    <button>Give Razzie Award!</button>
</form>

{{/each}}