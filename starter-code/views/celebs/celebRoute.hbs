<h2>Celebrity List</h2>
{{#each celebList}}
<p>{{this.name}}</p>
<a href="celebrities/details/{{this._id}}">
    <button>More Info!</button>
</a>
<a href="celebrities/edit/{{this._id}}">
    <button>Edit Celeb!</button>
</a>
<form action="celebrities/delete/{{this._id}}" method="POST">
    <button>Blacklist from Hollywood!</button>
</form>
</a>
{{/each}}